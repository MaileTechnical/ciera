  select one pkg related by s_ee->PE_PE[R8001]->EP_PKG[R8000];
  select one s_sys related by pkg->S_SYS[R1405];
  if ( s_sys.Name == "cairn" and pkg.Name == "util" )
    select any empty_comp_def from instances of ComponentDefinition where false;
    // modify the key letters of the EE
    s_ee.Key_Lett = "Default" + s_ee.Key_Lett;
    ::transformUtility( s_ee:s_ee, ep_pkg:pkg, containing_comp_def: empty_comp_def, exclude:true );
  end if;
end for;',
