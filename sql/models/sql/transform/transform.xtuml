INSERT INTO S_SYNC
	VALUES ("93bd40fe-7da0-4849-be47-fd3b21db1581",
	"00000000-0000-0000-0000-000000000000",
	'transformInstanceLoader',
	'',
	'c_c = param.c_c;

// create the instance inst_loader
create object instance inst_loader of InstanceLoader;
inst_loader.comp_name = ::camelCaseName( name:c_c.Name, start_lower:false );
inst_loader.name = inst_loader.comp_name + "InstanceLoader";
inst_loader.package = ::pathToPackage( path:param.path );
inst_loader.path = param.path;

// collect all contained classes
select many o_objs from instances of O_OBJ where false;
select many ep_pkgs related by c_c->PE_PE[R8003]->EP_PKG[R8001];
for each ep_pkg in ep_pkgs
  ::transformPackageForLoader( ep_pkg:ep_pkg, inst_loader:inst_loader, path:inst_loader.path + "/" + T::l( s:c_c.Name ) );
end for;',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'',
	0);
