<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>io.ciera</groupId>
  <artifactId>io.ciera.src</artifactId>
  <packaging>pom</packaging>
  <version>1.1.9-SNAPSHOT</version>
  <name>Ciera Source Bundle</name>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
    <bpLoc>${env.HOME}/git/bridgepoint</bpLoc>
    <mcLoc>${env.HOME}/git/xtuml2/mc</mcLoc>
  </properties>
  <scm>
    <connection>scm:git:git@github.com:leviathan747/ciera.git</connection>
    <developerConnection>scm:git:git@github.com:leviathan747/ciera.git</developerConnection>
    <tag>HEAD</tag>
  </scm>
  <distributionManagement>
    <repository>
      <id>io.ciera.repo</id>
      <url>https://maven.pkg.github.com/leviathan747/ciera</url>
    </repository>
  </distributionManagement>
  <modules>
    <module>io.ciera.runtime</module>
    <module>io.ciera.tool.core</module>
    <module>io.ciera.tool.sql</module>
    <module>io.ciera.tool.template</module>
    <module>ciera-maven-plugin</module>
  </modules>
</project>
