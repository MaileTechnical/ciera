<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>io.ciera</groupId>
  <artifactId>io.ciera.src</artifactId>
  <packaging>pom</packaging>
  <version>1.1.8-SNAPSHOT</version>
  <name>Ciera Source Bundle</name>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
  </properties>
  <repositories>
    <repository>
      <id>io.ciera.repo</id>
      <url>http://s3.amazonaws.com/xtuml-releases/maven</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <id>io.ciera.repo</id>
      <url>http://s3.amazonaws.com/xtuml-releases/maven</url>
    </pluginRepository>
  </pluginRepositories>
  <scm>
    <connection>scm:git:/Users/levi/gd/Git/ciera.git</connection>
    <developerConnection>scm:git:/Users/levi/gd/Git/ciera.git</developerConnection>
    <tag>HEAD</tag>
  </scm>
  <distributionManagement>
    <repository>
      <id>io.ciera.repo</id>
      <url>s3://xtuml-releases/maven</url>
    </repository>
  </distributionManagement>
  <build>
    <extensions>
      <extension>
        <groupId>io.github.leviathan747</groupId>
        <artifactId>s3-storage-wagon</artifactId>
        <version>1.6</version>
      </extension>
    </extensions>
  </build>
  <modules>
    <module>io.ciera.runtime</module>
    <module>io.ciera.tool.core</module>
    <module>io.ciera.tool.sql</module>
    <module>io.ciera.tool.template</module>
  </modules>
</project>
