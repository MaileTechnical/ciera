INSERT INTO O_TFR
	VALUES ("93bb4e59-a29f-44de-95ec-f24d3a9102ef",
	"77e6323b-cb89-4cc9-a176-7a026870d048",
	'render_relate',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'// Render a ''relate'' invocation.
index = 1;
newvar = TRUE;
select one serializer related by self->AssociationSerializer[R3023.''processes 2nd id position for''];
if ( empty serializer )
  index = 2;
  select one serializer related by self->AssociationSerializer[R3027.''processes 3rd id position for''];
  select one ref2 related by serializer->ParticipantReference[R3023.''renders code for 2nd id position using''];
  if ( self.class_name == ref2.class_name )
  	newvar = FALSE;
  end if;
end if;

form_class = serializer.formalizing_class_name;
formvar = form_class + STRING::itoa( i: self.rel_num );
part_class = self.class_name;
partvar = part_class +  STRING::itoa( i: self.rel_num ) + "_p";
rel_name = self.rel_name;

T::include(file:"loader/t.linkloader.ref.relate.java");',
	1,
	'',
	"3df2c5af-7188-4391-b92b-2022650ddff5",
	0,
	4);
